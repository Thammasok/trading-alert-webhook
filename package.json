{
  "name": "trading-alert-webhook",
  "version": "0.1.0",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start:js": "bun ./dist/index.js",
    "build": "tsdown api/index.ts api --dts",
    "build:js": "bun build --minify-whitespace --minify-syntax --outfile dist/index.js src/index.ts",
    "start:binary": "./dist/index",
    "build:binary": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile dist/index src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "elysia": "^1.4.16"
  },
  "devDependencies": {
    "bun-types": "^1.3.2",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3"
  }
}
